{
  "name": "comparemyrace-frontend",
  "version": "0.0.1",
  "description": "Web application to allow you to compare your races on Strava from previous years.",
  "main": "index.js",
  "repository": "https://github.com/blairg/comparemyrace-frontend",
  "author": "Blair Garrett",
  "license": "MIT",
  "scripts": {
    "webserver": "node api/src/server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js|jsx)' --require node_modules/ignore-styles --recursive",
    "test:coverage": "babel-node node_modules/.bin/babel-istanbul cover _mocha -- --require ./test/test_helper.js 'test/**/*.@(js|jsx)' --require node_modules/ignore-styles --recursive",
    "test:watch": "mocha -w --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js|jsx)'",
    "build": "node_modules/.bin/webpack",
    "dev": "DEBUG=true node_modules/.bin/webpack-dev-server",
    "lint": "eslint . --ignore-path .gitignore --ext .js --ext .jsx",
    "deploy": "NODE_ENV=production webpack -p",
    "clean": "rm -rf app/dist app/build",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "dependencies": {
    "css-loader": "^0.26.1",
    "empty": "^0.10.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "knex": "^0.11.10",
    "koa": "^1.2.1",
    "koa-cors": "0.0.16",
    "koa-route": "^2.4.2",
    "koa-router": "^5.4.0",
    "node-sass": "4.3.0",
    "react": "^15.3.2",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.1",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "coveralls": "^2.12.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.40.0",
    "html-webpack-plugin": "^2.24.1",
    "ignore-styles": "^5.0.1",
    "image-webpack-loader": "^3.3.0",
    "istanbul": "^0.4.5",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "engines": {
    "node": "7.3.0"
  }
}
